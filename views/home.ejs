<%-include("partials/header");-%>

<h1>Home</h1>

<p><%=homeStartingContent%></p>

<% posts.forEach(function(post) { %>

  <h1><%=post.title%></h1>
  <p>
    <% let path = "/posts/" + post.title; %>
    <%=post.content.substring(0, 100) + " ..."%>
    <a href="<%=path%>">Read more</a>
  </p>

<% }); %>

<%-include("partials/footer");-%>
